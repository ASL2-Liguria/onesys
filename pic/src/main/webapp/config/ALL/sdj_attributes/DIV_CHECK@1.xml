                <PAR type="idx" value="2"/>
            </PROC>
        </OBJ>
        <OBJ classe_id="PATH::div{.extCheck@EXT_CHECK}">
            <PROC id="setColSave">
                <PAR type="idx" value="5"/>
            </PROC>
        </OBJ>
        <OBJ classe_id="PATH::div{.extCheck@EXT_CHECK}">
            <PROC id="generaCheck">
                <PAR type="idx" value="0"/>
                <PAR type="idx" value="1"/>
            </PROC>
        </OBJ>
    </ATTR>
