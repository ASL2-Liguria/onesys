<?xml version="1.0" encoding="ISO-8859-1"?>
<WORKLIST>
    <ID>AC_PERSONALE</ID>
    <ID-QUERY connection="PORTALE_PIC">AUTOCOMPLETE.AC_PERSONALE</ID-QUERY>
    <TITLE>"AC Personale"</TITLE>
    <PAGER>S</PAGER>
    <SELECT_COLUMNS>S</SELECT_COLUMNS>
    <WHERE_BASE>IDEN=-1</WHERE_BASE>
    <STRUCTURE>
        <ROWS>
            <ALTERNATE_COLOR>S</ALTERNATE_COLOR>
            <HEIGHT>26px</HEIGHT>
            <EVENTS>
                <CLICK><![CDATA[function(rec){$(this).selectedLine('', 'clsSel'); $(this).trigger('select');}]]></CLICK>
            </EVENTS>
        </ROWS>
        <COLUMNS>
            <LIST>
                <COLUMN>
                    <ID>
                        <COL>DESCR</COL>
                        <DB>DESCR</DB>
                    </ID>
                    <DESCRIPTOR>traduzione.lblAcDescrizionePersonale</DESCRIPTOR>
                    <PROCESS/>
                    <WIDTH>150px</WIDTH>
                    <VISIBLE>S</VISIBLE>
                    <SORTABLE>S</SORTABLE>
                    <MOVABLE>S</MOVABLE>
                    <RESIZABLE>S</RESIZABLE>
                </COLUMN>
                <COLUMN>
                    <ID>
                        <COL>COD_DEC</COL>
                        <DB>COD_DEC</DB>
                    </ID>
                    <DESCRIPTOR>traduzione.lblAcCodicePersonale</DESCRIPTOR>
                    <PROCESS/>
                    <WIDTH>120px</WIDTH>
                    <VISIBLE>S</VISIBLE>
                    <SORTABLE>S</SORTABLE>
                    <MOVABLE>S</MOVABLE>
                    <RESIZABLE>S</RESIZABLE>
                </COLUMN>
            </LIST>
            <SORT>
                <COLUMN id="DESCR">ASC</COLUMN>
            </SORT>
        </COLUMNS>
    </STRUCTURE>
</WORKLIST>