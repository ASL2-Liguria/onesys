					</TR.INIT>
					<TR_TD_LABEL_CHECK key_id="chkTipoLOG" valore="lblTipoLOG##chkTipoLOG#chkTipoLOG#N#{width:280, ctrl:false}###9" evento="START"></TR_TD_LABEL_CHECK>
					<TR.INIT key_id="trButtonsSel" valore="" evento="START">
						<TD.INIT key_id="TD" valore="9" evento="START">
							<BUTTON key_id="butSelezionaTutti" valore="butSelezionaTutti" evento="START"></BUTTON>
							<BUTTON key_id="butDeselezionaTutti" valore="butDeselezionaTutti" evento="START"></BUTTON>
						</TD.INIT>			
					</TR.INIT>
				</FIELDSET_TABLE>
				<FIELDSET key_id="fldElencoLOG" valore="fldElencoLOG" evento="START">
					<DIV.WORKLIST key_id="divWkElencoLOG" valore="divWkElencoLOG" evento="START"></DIV.WORKLIST>				
				</FIELDSET>
			</TABULAZIONE.CREA_TAB>	
		</TABULAZIONE.INIT>
	</FORM>
	
</PAGINA>