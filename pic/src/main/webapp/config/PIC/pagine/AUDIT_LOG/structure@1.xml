<PAGINA key_id="AUDIT_LOG">
	
	<HEADER_PAGE key_id="AUDIT_LOG" valore="DEFAULT_PIC,SCHEDA,AUDIT_LOG#DEFAULT,AUDIT_LOG" evento="START_END">
	</HEADER_PAGE>
	
	<FORM key_id="dati" valore="dati##AUDIT_LOG##IDEN#IDEN##" evento="START_END">
		<TABULAZIONE.INIT key_id="tab" valore="Elenco_LOG#lblTitolo#butChiudi" evento="START">
			<TABULAZIONE.CREA_TAB key_id="tabElencoLOG" valore="tabElencoLOG" evento="START">
				<FIELDSET_TABLE key_id="fldRicerca" valore="fldRicerca"
					evento="START">
					<TR.INIT key_id="tr" valore="" evento="START">
						<TD_LABEL_DATA key_id="DaData"
							valore="lblDaData##txtDaData#DataInizio#startWithToday:false, direction:false, readonly_element: false, format: 'd/m/Y', start_date: -3#disabled:true#lblDaData#DATA_INIZIO"
							evento="START">
						</TD_LABEL_DATA>
						<TD_LABEL_COMBO key_id="cmbOraInizio" valore="lblOraInizio##cmbOraInizio#cmbOraInizio#N#ORA_INIZIO#S#" evento="START"></TD_LABEL_COMBO>
						<TD_LABEL_DATA key_id="AData"
							valore="lblAData##txtAData#DataFine#startWithToday:true, direction:false, readonly_element: false, format: 'd/m/Y'#disabled:true#lblAData#DATA_FINE"
							evento="START">
						</TD_LABEL_DATA>
						<TD_LABEL_COMBO key_id="cmbOraFine" valore="lblOraFine##cmbOraFine#cmbOraFine#N#ORA_FINE#S#" evento="START"></TD_LABEL_COMBO>
