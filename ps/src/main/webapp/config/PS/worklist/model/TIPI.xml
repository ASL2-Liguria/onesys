        </LIST>
        <SORT>
            <COLUMN id="TIPO">ASC</COLUMN>
            <COLUMN id="DESCRIZIONE">ASC</COLUMN>
        </SORT>
    </COLUMNS>
    </STRUCTURE>
</WORKLIST>