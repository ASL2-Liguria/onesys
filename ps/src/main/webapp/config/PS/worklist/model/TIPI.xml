<?xml version="1.0" encoding="ISO-8859-1"?>
<WORKLIST>
    <ID>TIPI</ID>
    <ID-QUERY connection="PS">WORKLIST.TIPI</ID-QUERY>
    <TITLE>"Worklist Tipi"</TITLE>
    <PAGER>S</PAGER>
    <SELECT_COLUMNS>S</SELECT_COLUMNS>
    <WHERE_BASE>IDEN=-1</WHERE_BASE>
