      <Attribute name="formula type" value="0"/>
      <Attribute name="basic syntax" value="false"/>
      <![CDATA[stringVar array farmaci_terapie="";
numbervar i;
Stringvar ret:="";

if isnull({Query.farmaci_terapie}) then(
""
)
else (
farmaci_terapie := Split({Query.farmaci_terapie},",");

ret:="<table style='width:100%;font-size:12px'>";
for i :=1 to Count (farmaci_terapie) step 1 do(

ret:=ret+"<tr><td>"+farmaci_terapie[i]+"</td></tr>";

);
ret:=ret+"</table>";
)]]>
