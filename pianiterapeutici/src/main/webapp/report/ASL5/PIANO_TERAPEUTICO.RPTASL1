    <Field type="FormulaField" valueType="6">
      <Attribute name="name" value="Età"/>
      <Attribute name="shortName" value="Età"/>
      <Attribute name="formula type" value="0"/>
      <Attribute name="basic syntax" value="false"/>
      <![CDATA[stringVar AnnoNascita :=  left({Query.PAZ_DATA_NASCITA},4);
stringVar MeseNascita :=  mid({Query.PAZ_DATA_NASCITA},5,2);
stringVar GiornoNascita :=  right({Query.PAZ_DATA_NASCITA},2);

/*converto le date in numeri*/

numbervar AnnoNascitaNumber := cdbl(AnnoNascita);
numbervar MeseNascitaNumber := cdbl(MeseNascita);
numbervar GiornoNascitaNumber := cdbl(GiornoNascita);
numberVar MeseGiornoNascitaNumber := cdbl(MeseNascita+GiornoNascita);

/*splitto la PAZ_DATA_NASCITA odierna*/
stringvar AnnoStringa :=cstr(year(currentdate),0);
stringvar MeseStringa :=cstr(Month(currentdate),0);
stringvar GiornoStringa :=cstr(day(currentdate),0);
      /* converto la PAZ_DATA_NASCITA adoerna in number*/

numbervar AnnoAttualeNumber := cdbl(AnnoStringa);
numbervar MeseAttualeNumber:= cdbl(MeseStringa);
numbervar GiornoAttualeNumber:= cdbl(GiornoStringa);

numbervar MeseGiornoAttualeNumber := cdbl(MeseStringa+GiornoStringa);

/*determino il risultato Standard*/
numbervar eta;

if (MeseGiornoAttualeNumber > MeseGiornoNascitaNumber ) then

eta := (AnnoAttualeNumber - AnnoNascitaNumber);

else

eta :=  (AnnoAttualeNumber - AnnoNascitaNumber) - 1;]]>
    </Field>
