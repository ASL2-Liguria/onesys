        <Sql>
          <![CDATA[select
   vpt.iden_testata
   ,vpt.paz_nome
   ,vpt.paz_cognome
   ,vpt.paz_data_nascita
   ,vpt.indicazione_lig
   ,vpt.paz_cod_fisc
   ,vpt.paz_indir
   ,vpt.paz_comune_residenza
   ,(select xxx_cusl 
from radsql.comuni 
where xxx_ccom = vpt.paz_comune_residenza) codice_asl_residenza
   ,(select xxx_crgn 
from radsql.comuni 
where xxx_ccom = vpt.paz_comune_residenza) codice_regione_residenza
   ,vpt.farmaci_terapie
   ,vpt.paz_sesso
   ,(select descrizione 
from radsql.tab_codifiche 
where codice = vpt.paz_asl_res) descrizione_asl
   ,vpt.posologia_oraria
   ,vpt.paz_asl_res
   ,vpt.posologia_durata
   ,vpt.posologia_unita
   ,vpt.posologia_periodo
   ,vpt.farmaco
   ,vpt.confezione
   ,vpt.principio_attivo
   ,vpt.diagnosi
   ,vpt.data_piano
   ,vpt.nota_aifa
   ,datetimeconverter(vpt.data_scadenza_piano,'yyyymmdd','dd/MM/yyyy') data_scadenza_piano
   ,datetimeconverter(vpt.data_piano,'yyyymmdd','dd/MM/yyyy') data_scadenza_piano
   ,datetimeconverter(vpt.data_piano,'yyyymmdd','dd/MM/yyyy') data_inizio_piano
   ,vpt.durata_trattamento
   ,(select cdc.descr 
from RADSQL.centri_di_costo cdc 
where cdc.cod_cdc = vpt.reparto ) reparto
   ,vpt.prescr_nome
   ,vpt.prescr_cognome
   ,vpt.prescr_cod_fisc
   ,vpt.sito
   ,vpt.descrizione_cdc
   ,vpt.descrizione_prov
from RADSQL.view_pt_rep vpt
where iden_testata = {?pIdenTestata}]]>
        </Sql>
      </Tablesource>
