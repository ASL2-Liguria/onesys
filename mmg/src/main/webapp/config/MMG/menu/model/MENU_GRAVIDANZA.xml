<MENU>
	<ID>MENU_GRAVIDANZA</ID>
	<TITLE>traduzione.lblMenu</TITLE>
	<LIST>
		<ITEM>
			<ID>MGRAV.I</ID>
			<LINK><![CDATA[function(rec){ NS_MENU_GRAVIDANZA.inserisciGravidanza( rec, $('#N_SCHEDA').val() ); } ]]></LINK>
			<ICON_CLASS>inserisci-esame</ICON_CLASS>
			<WHERE><![CDATA[function(rec){return true;}]]></WHERE>
			<OUTPUT>traduzione.lblInserisci</OUTPUT>
			<CONCEALING>false</CONCEALING>
			<SEPARATOR>false</SEPARATOR>
			<ENABLE>S</ENABLE>
			<INFO>Menu</INFO>
		</ITEM>
		<ITEM>
			<ID>MGRAV.M</ID>
			<LINK><![CDATA[function(rec){ NS_MENU_GRAVIDANZA.modificaGravidanza( rec, $('#N_SCHEDA').val() ); } ]]></LINK>
			<ICON_CLASS>modifica</ICON_CLASS>
			<WHERE><![CDATA[function(rec){return rec.length == 1;}]]></WHERE>
			<OUTPUT>traduzione.lblModifica</OUTPUT>
			<CONCEALING>true</CONCEALING>
			<SEPARATOR>false</SEPARATOR>
			<ENABLE>S</ENABLE>
			<INFO>Menu</INFO>
		</ITEM>
		<ITEM>
			<ID>MGRAV.D</ID>
			<LINK><![CDATA[function(rec){ NS_MENU_GRAVIDANZA.cancellaGravidanza( rec ); } ]]></LINK>
			<ICON_CLASS>modifica</ICON_CLASS>
			<WHERE><![CDATA[function(rec){return rec.length == 1;}]]></WHERE>
			<OUTPUT>traduzione.lblCancella</OUTPUT>
			<CONCEALING>true</CONCEALING>
			<SEPARATOR>false</SEPARATOR>
			<ENABLE>S</ENABLE>
			<INFO>Menu</INFO>
		</ITEM>
		<ITEM>
			<ID>MGRAV.CP</ID>
			<LINK><![CDATA[function(rec){ NS_MENU_GRAVIDANZA.inserisciDataParto( rec ); } ]]></LINK>
			<ICON_CLASS>data</ICON_CLASS>
			<WHERE><![CDATA[function(rec){return (rec[0].DATA_PARTO == '' || rec[0].DATA_PARTO == null);}]]></WHERE>
			<OUTPUT>traduzione.lblChiudiParto</OUTPUT>
			<CONCEALING>true</CONCEALING>
			<SEPARATOR>false</SEPARATOR>
            <ENABLE>S</ENABLE>
			<INFO>Menu</INFO>
		</ITEM>
	</LIST>
</MENU>