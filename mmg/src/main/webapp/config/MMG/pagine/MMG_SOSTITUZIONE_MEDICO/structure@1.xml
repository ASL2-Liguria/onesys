<PAGINA key_id="MMG_SOSTITUZIONE_MEDICO">
	<HEADER_PAGE key_id="MMG_SOSTITUZIONE_MEDICO"
		valore="DEFAULT_MMG,SCHEDA,MMG_SOSTITUZIONE_MEDICO#DEFAULT_MMG,MMG_SOSTITUZIONE_MEDICO"
		evento="START_END">
	</HEADER_PAGE>
	<FORM key_id="dati"
		valore="dati#SAVE_SOSTITUZIONE_MEDICO#MMG_DATI#MMG_MEDICO_SOSTITUZIONI#IDEN#IDEN##VSOSTITUZIONE_MEDICO"
		evento="START_END">
		<TABULAZIONE.INIT key_id="tabSostMedico"
			valore="SostituzioneMedico#lblTitolo#butSalva,butChiudi" evento="START_END">
			<TABULAZIONE.CREA_TAB key_id="tabSostituzione" valore="tabSostituzione" evento="START">
				<FIELDSET_TABLE key_id="fldPeriodo" valore="fldPeriodo"
					evento="START">
					<TR_TD_LABEL_DATA key_id="DaData"
						valore="lblDaData##txtDaData#DataInizio#startWithToday:true, readonly_element: false, format: 'd/m/Y',onSelect: function(){}#disabled:false##DATA_INIZIO"
						evento="START">
					</TR_TD_LABEL_DATA>
					<TR_TD_LABEL_DATA key_id="AData"
						valore="lblAData##txtAData#DataFine#startWithToday:false, readonly_element: false, format: 'd/m/Y',onSelect: function(){}#disabled:false##DATA_FINE"
						evento="START">
					</TR_TD_LABEL_DATA>
				</FIELDSET_TABLE>
				<FIELDSET_TABLE key_id="fldSostituzione" valore="fldSostituzione"
					evento="START">
					<INPUT.HIDDEN key_id="hMedicoBase" valore="hMedicoBase###IDEN_MED"
						evento="START">
					</INPUT.HIDDEN>
					<INPUT.HIDDEN key_id="hIden" valore="hIden###IDEN"
						evento="START">
					</INPUT.HIDDEN>
					<TR_TD_AUTOCOMPLETE key_id="Sostituto"
						valore="lblMedicoSostituto#acMedicoSostituto#Sostituto#txtDescrSostituto#AC_MEDICO_SOSTITUTO#IDEN_MED_SOSTITUTO"
						evento="START">
					</TR_TD_AUTOCOMPLETE>
					<TR_TD_LABEL_RADIO key_id="Liquid"
						valore="lblLiquid##Liquid#radLiquid#{width:'auto'}#N#LIQUID"
						evento="START">
					</TR_TD_LABEL_RADIO>
					<TR_TD_LABEL_RADIO key_id="Visione"
						valore="lblVisione##Visione#radVisione#{width:'auto'}#S#VISIONE"
						evento="START">
					</TR_TD_LABEL_RADIO>
					<TR_TD_LABEL_RADIO key_id="DatiGruppo"
						valore="lblDatiGruppo##DatiGruppo#radDatiGruppo#{width:'auto'}#S#SOST_GRUPPO"
						evento="START">
					</TR_TD_LABEL_RADIO>
					<TR_TD_LABEL_DATA key_id="DataIniVisione"
						valore="lblDataIniVisione##txtDataIniVisione#DataIniVisione#startWithToday:false, readonly_element: false, format: 'd/m/Y',onSelect: function(){}#disabled:false##DATA_INI_VISIONE"
						evento="START">
					</TR_TD_LABEL_DATA>
				</FIELDSET_TABLE>
				<FIELDSET_TABLE key_id="fldMotivo" valore="fldMotivo"
					evento="START">
					<TR_TD_TEXTAREA key_id="MotivoSostituzione" valore="txtAreaMotivo#NOTE#4"
						evento="START">
					</TR_TD_TEXTAREA>
				</FIELDSET_TABLE>
			</TABULAZIONE.CREA_TAB>
		</TABULAZIONE.INIT>
	</FORM>
</PAGINA>

