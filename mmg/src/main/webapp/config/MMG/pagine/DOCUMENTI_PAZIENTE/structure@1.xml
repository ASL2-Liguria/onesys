<PAGINA key_id="DOCUMENTI_PAZIENTE">
	<HEADER_PAGE key_id="DOCUMENTI_PAZIENTE"
		valore="DEFAULT_MMG,SCHEDA,DOCUMENTI_PAZIENTE#DEFAULT_MMG,DOCUMENTI_PAZIENTE"
		evento="START_END">
	</HEADER_PAGE>
	<FORM key_id="dati"
		valore="dati#SAVE_DATA#MMG_DATI#DOCUMENTI_PAZIENTE#IDEN#IDEN" evento="START_END">
		<TABULAZIONE.INIT key_id="tabber"
			valore="DocumentiPaziente#lblTitolo#butChiudi" evento="START">
			<TABULAZIONE.CREA_TAB key_id="tabDocRepository"
				valore="tabDocRepository" evento="START">
				<FIELDSET_TABLE key_id="fldRicerca" valore="fldRicerca"
					evento="START">
					<TR.INIT key_id="tr" valore="" evento="START">
						<TD_LABEL_DATA key_id="DaData"
							valore="lblDaData##txtDaData#DataInizio#startWithToday:false, direction:false, readonly_element: false, format: 'd/m/Y', start_date: -3#disabled:false#lblDaData#DATA_INIZIO"
							evento="START">
						</TD_LABEL_DATA>
						<TD_LABEL_DATA key_id="AData"
							valore="lblAData##txtAData#DataFine#startWithToday:true, readonly_element: false, format: 'd/m/Y'#disabled:false#lblAData#DATA_FINE"
							evento="START">
						</TD_LABEL_DATA>
					</TR.INIT>
					<TR_TD_LABEL_CHECK key_id="Ricerca"
						valore="lblRicerca##Ricerca#chkRicerca#N#{width:150}###4" evento="START">
					</TR_TD_LABEL_CHECK>
					<TR.INIT key_id="TR" valore="" evento="START">
						<TD_LABEL key_id="lbl_" valore="lbl_##" evento="START">></TD_LABEL>
						<TD.INIT key_id="TDLBL" valore="2" evento="START">
							<LABEL key_id="lblUltimi5" valore="lblUltimi5#lblUltimi5" evento="START">></LABEL>
						</TD.INIT>
						<TD.INIT key_id="TD" valore="4" evento="START">
							<BUTTON key_id="butUltimi5" valore="butUltimi5" evento="START"></BUTTON>
							<BUTTON key_id="butCerca" valore="butCerca" evento="START"></BUTTON>
						</TD.INIT>
					</TR.INIT>
					<INPUT.HIDDEN key_id="Cod_Fisc" valore="Cod_Fisc"
						evento="START">
					</INPUT.HIDDEN>
				</FIELDSET_TABLE>
				<DIV.WORKLIST key_id="ElencoWork" valore="ElencoWork"
					evento="START">
				</DIV.WORKLIST>
			</TABULAZIONE.CREA_TAB>
			<TABULAZIONE.CREA_TAB key_id="tabDocAllegati"
				valore="tabDocAllegati" evento="START">
				<FIELDSET_TABLE key_id="fldAllegato" valore="fldAllegato"
					evento="START">
					<TR_TD_LABEL_FILE key_id="AllegaFile"
						valore="lblAllegaFile##AllegaFile" evento="START">
					</TR_TD_LABEL_FILE>
					<TR_TD_LABEL_TEXT key_id="NomeAllegato"
						valore="lblNomeAllegato##txtNomeAllegato###NOME_FILE" evento="START">
					</TR_TD_LABEL_TEXT>
					<INPUT.HIDDEN key_id="hFileAllegato" valore="hFileAllegato"
						evento="START">
					</INPUT.HIDDEN>
					<TR_TD_LABEL_RADIO key_id="AllegaPazienteEpisodio"
						valore="lblAllegaPazienteEpisodio##AllegaPazienteEpisodio#radAllegaPazienteEpisodio#{width:150}#N"
						evento="START">
					</TR_TD_LABEL_RADIO>
					<TR_TD_LABEL_COMBO key_id="cmbTipoDocumento"
						valore="lblTipoDocAllegato##cmbTipoDocAllegato#cmbTipoDocAllegato##"
						evento="START">
					</TR_TD_LABEL_COMBO>
					<TR.INIT key_id="tr1" valore="" evento="START">
						<TD.INIT key_id="td1" valore="2#right-align" evento="START">
							<BUTTON key_id="butSalva" valore="butSalva" evento="START">
							</BUTTON>
							<BUTTON key_id="butImportaDaScanner" valore="butImportaDaScanner"
								evento="START">
							</BUTTON>
						</TD.INIT>
					</TR.INIT>
				</FIELDSET_TABLE>
				<DIV.WORKLIST key_id="divWk" valore="divWk" evento="START">
				</DIV.WORKLIST>
			</TABULAZIONE.CREA_TAB>
		</TABULAZIONE.INIT>
	</FORM>
</PAGINA>

