<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ATTRS>
    <ATTR ev="START" id="ROW">
        <OBJ classe_id="PATH::div*">
            <PROC id="addAttribute">
                <PAR value="class"/>
                <PAR value="row"/>
            </PROC>
        </OBJ>
        <OBJ classe_id="PATH::div*">
            <PROC id="setId">
                <PAR type="idx" value="0"/>
            </PROC>
        </OBJ>
        <OBJ classe_id="PATH::div*">
            <PROC id="appendAttribute">
                <PAR value="class"/>
                <PAR type="idx" value="1"/>
            </PROC>
        </OBJ>
    </ATTR>
</ATTRS>
