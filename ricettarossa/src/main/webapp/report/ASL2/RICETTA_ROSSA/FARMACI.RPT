        <Sql>
          <![CDATA[select 
   rrt.iden iden_testata,  
   a.iden,  a.nome nome_paziente, a.cogn cognome_paziente, a.cod_fisc cod_fisc_paziente, a.indir, a.com_nasc,a.com_res, a.cusl, a.prov,
   to_char(rrt.data_ricetta, 'ddMMyy') data_ricetta, rrt.iden, rrt.cod_bar_1, rrt.cod_bar_2, c.comune,
   tp.nome nome_medico, tp.cognome cognome_medico, tp.cod_fisc cod_fisc_medico,rrt.quesito,
  (select  extractvalue(contenuto,'/PAGINA/CAMPI/CAMPO[@KEY_CAMPO="txtDescrizioneICD"]') 
from rr_ricetta_rossa_scheda rrs 
where rrs.progressivo=rrt.progressivo) descr_icd,
   concat_fields('select f.descrizione||''$$''||to_char(d.scatole)||''$$''||d.posologia as concat 
from radsql.view_rr_farmaci f inner join  radsql.rr_ricetta_rossa_dettaglio d on (f.iden = d.iden_prescrizione) 
where d.iden_testata = '||rrt.iden ,chr(64)) concatenazione_farmaci
   ,rrt.codice_reddito,
   substr(rrt.codice_reddito,1,1) cod1,
   substr(rrt.codice_reddito,2,1) cod2,
   substr(rrt.codice_reddito,3,1) cod3,
   substr(rrt.codice_reddito,4,1) cod4,
   substr(rrt.codice_reddito,5,1) cod5,
   substr(rrt.codice_reddito,6,1) cod6,rre.codice,rre.cod_farma,
   (select decode (categoria_ricetta, 'SSN', '') 
from RADSQL.rr_ricetta_rossa_testata 
where iden=rrt.iden) categoria_ricetta,
   (select concat_fields('select descrizione 
from radsql.view_rr_farmaci 
where iden in (select iden_prescrizione 
from radsql.rr_ricetta_rossa_dettaglio 
where iden_testata in '||rrt.iden||')',chr(36))from dual) farmaci,
   (select concat_fields('select nota_cuf 
from radsql.rr_ricetta_rossa_dettaglio 
where iden_testata = '||rrt.iden ,chr(36))from dual) nota_cuf,
   (select sum(scatole) 
from RADSQL.rr_ricetta_rossa_dettaglio 
where iden_testata = rrt.iden) as n_scatole,
   rrt.cod_bar_1||rrt.cod_bar_2 cod_interno_identificativo
from radsql.rr_ricetta_rossa_testata rrt 
   left join RADSQL.view_rr_esenzioni rre on (rre.iden = rrt.iden_esenzione)
   --left join radsql.rr_ricetta_rossa_dettaglio rrd on (rrd.iden_testata = rrt.iden)
   inner join radsql.anag a on (a.iden = rrt.iden_anag)
   left join radsql.comuni c on (a.com_res = c.xxx_ccom or a.com_res = c.xxx_ccom)
   inner join radsql.tab_per tp on (tp.iden = rrt.iden_medico)   
where rrt.iden =  {?pIdenTestata}]]>
        </Sql>
